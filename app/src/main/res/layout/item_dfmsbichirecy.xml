<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="28px"
    android:orientation="horizontal">

    <ImageView
        android:id="@+id/item_dfmsbichiImage"
        android:layout_width="145px"
        android:layout_height="145px"
        android:layout_marginLeft="28px"
        android:scaleType="fitXY" />

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/item_dfmsbichiNameText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="36px"
            android:layout_marginTop="39px"
            android:text="苏四羊杂碎"
            android:textColor="#303030"
            android:textSize="24px" />

        <com.zhy.autolayout.AutoRelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="36px"
            android:layout_marginTop="24px">

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/item_dfmsbichiStartOne"
                    android:layout_width="25px"
                    android:layout_height="25px"
                    android:src="@mipmap/start" />

                <ImageView
                    android:id="@+id/item_dfmsbichiStartTwo"
                    android:layout_width="25px"
                    android:layout_height="25px"
                    android:layout_marginRight="5px"
                    android:src="@mipmap/start" />

                <ImageView
                    android:id="@+id/item_dfmsbichiStartThree"
                    android:layout_width="25px"
                    android:layout_height="25px"
                    android:layout_marginRight="5px"
                    android:src="@mipmap/start" />

                <ImageView
                    android:id="@+id/item_dfmsbichiStartFour"
                    android:layout_width="25px"
                    android:layout_height="25px"
                    android:layout_marginRight="5px"
                    android:src="@mipmap/start" />

                <ImageView
                    android:id="@+id/item_dfmsbichiStartFive"
                    android:layout_width="25px"
                    android:layout_height="25px"
                    android:layout_marginRight="5px"
                    android:src="@mipmap/start" />

                <TextView
                    android:id="@+id/item_dfmsbichiTypeText"
                    android:layout_width="104px"
                    android:layout_height="24px"
                    android:layout_marginLeft="44px"
                    android:background="@drawable/youhui_btn"
                    android:gravity="center"
                    android:text="小吃快餐"
                    android:textColor="#ffffff"
                    android:textSize="18px" />
            </com.zhy.autolayout.AutoLinearLayout>


            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/item_dfmsbichiKStartOne"
                    android:layout_width="25px"
                    android:layout_height="25px"
                    android:src="@mipmap/kongstart" />

                <ImageView
                    android:id="@+id/item_dfmsbichiKStartTwo"
                    android:layout_width="25px"
                    android:layout_height="25px"
                    android:layout_marginRight="5px"
                    android:src="@mipmap/kongstart" />

                <ImageView
                    android:id="@+id/item_dfmsbichiKStartThree"
                    android:layout_width="25px"
                    android:layout_height="25px"
                    android:layout_marginRight="5px"
                    android:src="@mipmap/kongstart" />

                <ImageView
                    android:id="@+id/item_dfmsbichiKStartFour"
                    android:layout_width="25px"
                    android:layout_height="25px"
                    android:layout_marginRight="5px"
                    android:src="@mipmap/kongstart" />

                <ImageView
                    android:id="@+id/item_dfmsbichiKStartFive"
                    android:layout_width="25px"
                    android:layout_height="25px"
                    android:layout_marginRight="5px"
                    android:src="@mipmap/kongstart" />
            </com.zhy.autolayout.AutoLinearLayout>
        </com.zhy.autolayout.AutoRelativeLayout>

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="5"
            android:layout_marginLeft="36px"
            android:layout_marginTop="24px"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/item_dfmsbichiAddressText"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"

                android:text="银川·西夏区"
                android:textColor="#8c8c8c"
                android:textSize="18px" />

            <TextView
                android:id="@+id/item_dfmsbichiPriceText"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"

                android:text="26/人"
                android:textColor="#8c8c8c"
                android:textSize="18px" />

            <com.zhy.autolayout.AutoLinearLayout
                android:id="@+id/item_dfmsbichiCommentButton"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="30px"

                    android:src="@mipmap/comment" />

                <TextView
                    android:id="@+id/item_dfmsbichiCommentText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="14px"
                    android:text="56"
                    android:textColor="#8c8c8c"
                    android:textSize="18px" />
            </com.zhy.autolayout.AutoLinearLayout>


            <TextView
                android:id="@+id/item_dfmsbichiDistance"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="28px"
                android:layout_weight="1"
                android:gravity="right"
                android:maxLines="1"
                android:singleLine="true"
                android:text="4.6KM"
                android:textColor="#8c8c8c"
                android:textSize="18px" />

        </com.zhy.autolayout.AutoLinearLayout>
    </com.zhy.autolayout.AutoLinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:background="#303030" />
</com.zhy.autolayout.AutoLinearLayout>